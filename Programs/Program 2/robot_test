echo "Test X: (shortest distance, number of paths)"
echo "TEST 1: (2,2)"
./greedy_robot 1 3 4 4 3
echo "TEST 2: (Already there)"
./greedy_robot 2 1 1 1 1
echo "TEST 3: (NP, 0)"
./greedy_robot 1 1 1 5 1
#
# Additional Test Cases
#
echo "TEST 4: (Simple move, 1 path)"
./greedy_robot 2 0 0 1 0
echo "TEST 5: (Simple move, 1 path)"
./greedy_robot 2 0 0 0 1
echo "TEST 6: (Multiple paths, 2, 2)"
./greedy_robot 2 0 0 2 2
echo "TEST 7: (Longer path, 3, 3)"
./greedy_robot 3 0 0 3 3
echo "TEST 8: (Edge case, max distance 1)"
./greedy_robot 1 0 0 1 1
echo "TEST 9: (Longer path, max distance 2)"
./greedy_robot 2 0 0 4 4
echo "TEST 10: (Negative coordinates)"
./greedy_robot 2 -1 -1 -3 -3
echo "TEST 11: (Mixed coordinates)"
./greedy_robot 2 1 -1 -1 1
echo "TEST 12: (Edge case, max distance 3)"
./greedy_robot 3 0 0 3 3
echo "TEST 13: (Edge case, max distance 4)"
./greedy_robot 4 0 0 4 4
echo "TEST 14: (Edge case, max distance 5)"
./greedy_robot 5 0 0 5 5
